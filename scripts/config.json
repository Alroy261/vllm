'{
   "model_id":"vllm_alroy",
   "deployment_spec":{
      "pod_spec":{
         "container_spec":{
            "registry":"public.ecr.aws/s5a5m7v8",
            "name":"vllm_alroy",
            "tag":"latest",
            "ports":[
               {
                  "name":"http",
                  "port":8000
               }
            ]
         },
         "resource":{
            "requests":{
               "cpu":"100m",
               "memory":"200Mi"
            },
            "limits":{
               "cpu":"500m",
               "memory":"10Gi"
            }
         },
         "node_affinity":{
            "required":{
               "node_selector_terms":[
                  {
                     "match_expressions":[
                        {
                           "key":"gpu.nvidia.com/class",
                           "operator":"In",
                           "values":[
                              "A5000"
                           ]
                        }
                     ]
                  }
               ]
            }
         }
      }
   },
   "service_spec":{
      "ports":[
         {
            "name":"default",
            "port":8000,
            "target_port":"http"
         }
      ]
   },
   "hpa_spec":{
      "min_replicas":1,
      "max_replicas":1,
      "target_cpu_utilization":50
   }
}'
